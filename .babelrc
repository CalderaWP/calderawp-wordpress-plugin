{
  "presets": [
    "@wordpress/default",
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-flow"
  ],
  "plugins": [
    [
      "@wordpress/babel-plugin-makepot",
      {
        "output": "languages/dropit.pot"
      },
      "transform-flow-comments"
    ],
    ["transform-es2015-modules-commonjs", {
      "allowTopLevelThis": true
    }]
  ]
}
