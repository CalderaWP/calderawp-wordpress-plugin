{
  "packageName": "calderaWordPressPlugin",
  "namespace": "caldera-wordpress-plugin",
  "plugins": [
    {
      "title" : "Layout Preview Settings",
      "slug" : "layout"
    }
  ],
  "blocks":
    [
      {
        "title": "Caldera Forms Entry",
        "slug": "entry",
        "icon": "editor-justify",
        "category": "common",
        "wpDependencies": [
          "editor",
          "components"
        ],
        "attributes": {
          "formId": {
            "type": "string",
            "default": ""
          },
          "entryId": {
            "type": "number",
            "default": 0
          }
        }
      },
      {
        "title": "Caldera Forms Entry Field",
        "slug": "entryValue",
        "category": "common",
        "wpDependencies": [
          "editor",
          "components"
        ],
        "attributes": {
          "formId": {
            "type": "string",
            "default": ""
          },
          "entryId": {
            "type": "number",
            "default": 0
          },
          "fieldId": {
            "type": "string",
            "default": ""
          },
          "before": {
            "type" : "string",
            "default": ""
          },
          "after": {
            "type" : "string",
            "default": ""
          }
        }
      },
      {
        "title": "Caldera Forms Entry Table",
        "slug": "entryTable",
        "icon": "editor-justify",
        "category": "common",
        "wpDependencies": [
          "editor",
          "components"
        ],
        "attributes": {
          "formId": {
            "type": "string",
            "default": ""
